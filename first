vae n=wionw.peompt("请输入一个整数");
n=parselnt(n);
var res=0;
var temp=1;
for(var i=1;i<=n;i++){
for(var j=1;j<=i;j++){
temp*=j;
}
res+=temp;
temp=1
}
 document.writeln("第二个结果是"+res);

